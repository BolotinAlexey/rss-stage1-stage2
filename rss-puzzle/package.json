{
  "name": "rss-puzzle",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.base.config.js",
  "scripts": {
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint --fix ./src/**/*.ts",
    "format": "prettier . --write",
    "ci:format": "prettier . --check",
    "serve": "webpack serve --open --config ./webpack.config.js --env mode=dev",
    "prepare": "cd .. && husky rss-puzzle/.husky",
    "build": "webpack --config ./webpack.config.js --env mode=prod"
  },
  "validate-branch-name": {
    "pattern": "^(feat|fix|refactor|chore)/RSS-PZ-\\d+_[a-zA-Z0-9-]+$",
    "errorMsg": "Wrong branch name"
  },
  "keywords": [],
